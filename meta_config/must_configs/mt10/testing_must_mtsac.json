{
    "env_name" : "mt10",
    "env":{
        "reward_scale":1,
        "obs_norm":false
    },
    "meta_env":{
        "obs_type": "with_goal_and_id"
    },
    "replay_buffer":{
        "size": 1e6
    },
    "net":{ 
        "hidden_shapes": [10,10]
        
    },
    "task_embedding":{
        "em_hidden_shapes":[10, 10]
    },
    "traj_encoder":{
        "hidden_shapes": [10,10],
        "latent_size": 10
    },
    "sparse_training":{
        "pruning_ratio":0.5

    }
    ,
    "general_setting": {
        "discount" : 0.99,
        "pretrain_epochs" : 0,
        "num_epochs" : 80,
        "epoch_frames" : 150,
        "max_episode_frames" : 150,

        "batch_size" : 1280,
        "min_pool" : 10000,

        "target_hard_update_period" : 1000,
        "use_soft_update" : true,
        "tau" : 0.005,
        "opt_times" : 5,
        "mask_update_interval": 2, 
        "update_end_epoch":3750,
        "eval_episodes" : 3
    },
    "sac":{
        
        "plr" : 3e-4,
        "qlr" : 3e-4,
        
        "reparameterization": true,
        "automatic_entropy_tuning": true,
        "policy_std_reg_weight": 0,
        "policy_mean_reg_weight": 0
    }
}
